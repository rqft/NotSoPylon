                            "level": {
                                "title": "Bot Level",
                                "type": "integer",
                                "minimum": 0,
                                "maximum": 999,
                                "default": 10
                            }
                        }
                    }
                },
                "users": {
                    "type": "array",
                    "format": "table",
                    "uniqueItems": "true",
                    "items": {
                        "type": "object",
                        "title": "Users",
                        "properties": {
                            "id": {
                                "title": "User ID",
                                "type": "string",
                                "default": "User ID",
                                "minLength": "16",
                                "maxLength": "20",
                                "pattern": "^[0-9]+$"
                            },
                            "level": {
                                "title": "Bot Level",
                                "type": "integer",
                                "minimum": -1,
