{
  "name": "NotSoPylon",
  "version": "0.0.6",
  "description": "yea",
  "scripts": {
    "typings": "typings install",
    "build": "npm run typings && rollup -c rollup.config.js",
    "publish": "node cd/publish.js",
    "lint": "eslint src/ --ext .ts --max-warnings 99999 --config .eslintrc",
    "lintfix": "eslint src/ --ext .ts --fix --config .eslintrc --quiet",
    "test": "rollup -c rollup_test.config.js && npm run publish",
    "testlint": "npm run lintfix && rollup -c rollup_test.config.js && npm run publish",
    "public": "rollup -c rollup_public.config.js"
  },
  "author": "rqft",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^3.0.0",
    "@rollup/plugin-strip": "^2.0.0",
    "@rollup/plugin-typescript": "^8.0.0",
    "@types/node": "^16.7.8",
    "babel-eslint": "^10.1.0",
    "detritus-client": "^0.16.2",
    "detritus-pagination": "^1.4.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.6.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "json-ts": "^1.6.4",
    "node-fetch": "^3.0.0",
    "rollup": "^2.52.0",
    "rollup-plugin-ignore": "^1.0.6",
    "rollup-plugin-terser": "^7.0.0",
    "typescript": "^4.0.5",
    "typescript-tools": "^0.3.1",
    "typings": "^2.1.1",
    "ws": "^8.0.0"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^4.0.0",
    "@typescript-eslint/parser": "^3.8.0",
    "cloc": "^2.8.0",
    "install": "^0.13.0",
    "js-yaml": "^4.0.0",
    "npm": "^7.5.6",
    "path": "^0.12.7",
    "sleep": "^6.3.0",
    "typescript-tslint-plugin": "^1.0.0"
  }
}
